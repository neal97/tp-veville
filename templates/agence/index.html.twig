{% extends 'base.html.twig' %}

{% block title %}Hello AgenceController!{% endblock %}

{% block body %}


<table class="table">
  <thead>
    <tr>
      <th scope="col">Vehicule</th>
      <th scope="col">Agence</th>
      <th scope="col">titre</th>
      <th scope="col">marque</th>
      <th scope="col">modele</th>
      <th scope="col">photo</th>
      <th scope="col">prix</th>
      <th scope="col">action</th>
    </tr>
  </thead>
</table>
{% for agence in agences %}

<tr>
    <td>{{ agence.id }}</td>
    <td>{{ agence.titre }}</td>
    <td>{{ agence.marque }}</td>
    <td>{{ agence.modele }}</td>
    <td>{{ agence.photo }}</td>
    <td>{{ agence.prix }}</td>
    <td>{{ agence.action }}</td>
</tr>


{% endfor %} 
{{ form_start(formAgence) }}



{{ form_end(formAgence) }}
{% endblock %}
